<launch>
  <!-- Note how the map is loaded. Use this to load other maps -->
  <arg name="map_file" default="$(find comp0037_resources)/maps/square_terrain.yaml"/>
  <node pkg="map_server" name="map_server" type="map_server" args="$(arg map_file)"/>
  <node pkg="comp0037_planner_controller" output='screen' name="fifo_standalone_node" type="fifo_standalone_node.py"/>
</launch>

  
