<?xml version="1.0"?>
<launch>
                        <param name="use_sim_time" type="bool" value="true" />
  <rosparam param="clock_send_rate">40</rosparam>
  <rosparam param="time_scale_factor">1</rosparam>

                <arg name="map" default="$(find comp313p_cw1)/scenarios/s02_factory/s02_factory.yaml"/>
                <arg name="goal_file" default="$(find comp313p_cw1)/scenarios/s02_factory/s02_goals.txt"/>
 

	<include file="$(find stdr_robot)/launch/robot_manager.launch" />
	
	<node type="stdr_server_node" pkg="stdr_server" name="stdr_server" output="screen" args="$(arg map)"/>

	<node pkg="tf" type="static_transform_publisher" name="world2map" args="0 0 0 0 0 0  world map 100" />
	
	<include file="$(find stdr_gui)/launch/stdr_gui.launch"/>
	
	<node pkg="stdr_robot" type="robot_handler" name="$(anon robot_spawn)" args="add $(find stdr_resources)/resources/robots/pandora_robot.yaml 1 2 0" />
	
  <node pkg="comp313p_time_server" name="time_server" type="time_server_node.py" output="screen"/>
  <node pkg="comp313p_the_boss" name="the_boss" type="boss_node.py" args="$(arg goal_file)" output="screen"/>
  <node pkg="comp313p_planner_controller" name="planner_node" type="planner_node.py" output="screen"/>

</launch>

  
